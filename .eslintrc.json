{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:json/recommended", "plugin:vue/vue3-essential", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "vue/multi-word-component-names": "off"
  }
}